package anderson.tankwar;

import java.awt.Graphics;

public class Wall {
	int x,y,w,h;
	TankClient tc;
	public Wall(int x, int y, int w, int h, TankClient tc) {
		super();
		this.x = x;
		this.y = y;
		this.w = w;
		this.h = h;
		this.tc = tc;
	}
	public void draw(Graphics g){
		g.fillRect(x, y, w, h);
	}
}
